{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "chika",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.chika",
					"match": "\\b(if|else|return)\\b"
				},
				{
					"name": "keyword.function.chika",
					"match": "\\b(fn)\\b"
				}
			]
		}
	},
	"scopeName": "source.chika"
}